	Journal = {The Annals of Statistics},
	Number = {4},
	Pages = {1100--1120},
	Title = {Cox's Regression Model for Counting Processes: A Large Sample Study},
	Url = {http://www.jstor.org/stable/2240714},
	Volume = {10},
	Year = {1982},
	Bdsk-File-1 = {},
