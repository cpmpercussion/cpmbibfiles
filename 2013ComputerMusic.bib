	Title = {Skill in Interactive Digital Music Systems},
	Year = {2014},
	Bdsk-Url-1 = {//www.oxfordhandbooks.com/10.1093/oxfordhb/9780199797226.001.0001/oxfordhb-9780199797226-e-019},
